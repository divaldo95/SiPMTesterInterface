{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MeasurementService": {
    "BlockCount": 2,
    "ModuleCount": 2,
    "ArrayCount": 4,
    "SiPMCount": 16
  },
  "AllowedHosts": "*",
  "NIMachine": {
    "IP": "127.0.0.1",
    "CommandPort": 5555,
    "LogPort": 5557,
    "Enabled": true,
    "UpdatePeriodms": 15000
  },
  "Pulser": {
    "Enabled": false,
    "AutoDetect": false,
    "SerialPort": "/dev/ttyUSB0",
    "BaudRate": 115200,
    "Timeout": 10000
  },
  "HVPSU": {
    "Enabled": false,
    "AutoDetect": false,
    "SerialPort": "/dev/ttyUSB1",
    "BaudRate": 115200,
    "Timeout": 10000
  },
  "DefaultMeasurementSettings": {
    "DMMResistance": {
      "Voltage": 30.0,
      "Iterations": 5,
      "CorrectionPercentage": 10
    },
    "IV": {
      "CurrentLimit": 0.01,
      "CurrentLimitRange": 0.01,
      "LED": 100
    }
  }
}